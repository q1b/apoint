{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/azure-swa/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postinstall": "prisma generate",
    "preview": "qwik build preview && vite preview --open",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "serve": "swa start",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@azure/functions": "^3.5.0",
    "@azure/static-web-apps-cli": "^1.1.1",
    "@builder.io/qwik": "^1.2.7",
    "@builder.io/qwik-city": "^1.2.7",
    "@builder.io/qwik-react": "^0.5.0",
    "@internationalized/date": "^3.4.0",
    "@prisma/client": "^5.2.0",
    "@qwikest/icons": "^0.0.8",
    "@tailwindcss/forms": "^0.5.4",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/eslint": "8.44.1",
    "@types/node": "^20.4.5",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "autoprefixer": "^10.4.14",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^2.0.0",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "^1.2.7",
    "just-memoize": "^2.2.0",
    "nanoid": "^4.0.2",
    "ordinal": "^1.0.3",
    "postcss": "^8.4.23",
    "prettier": "3.0.1",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-aria": "^3.26.0",
    "react-aria-components": "^1.0.0-alpha.6",
    "react-dom": "18.2.0",
    "react-stately": "^3.24.0",
    "seroval": "^0.9.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "unstorage": "^1.8.0",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "@googleapis/calendar": "^9.1.1",
    "@lucia-auth/adapter-prisma": "^3.0.1",
    "@lucia-auth/oauth": "^2.1.2",
    "@modular-forms/qwik": "^0.20.0",
    "a11y-dialog": "^8.0.1",
    "just-group-by": "^2.2.0",
    "just-order-by": "^1.0.0",
    "lucia": "^2.3.0",
    "sha1-uint8array": "^0.10.7"
  }
}
